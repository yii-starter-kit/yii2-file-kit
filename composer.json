{
    "name": "trntv/yii2-file-kit",
    "description": "Yii2 file upload and storage kit",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Eugine Terentev",
            "email": "eugine@terentev.net"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-jui": "*",
        "jakeasmith/http_build_url": "*"
    },
    "autoload": {
        "psr-4": { "trntv\\filekit\\": "" }
    },
    "suggest": {
        "ext-http": "HTTP PHP Extension"
    },
    "extra":{
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        },
        "bootstrap": "trntv\\filekit\\storage\\FileStorage"
    }
}
